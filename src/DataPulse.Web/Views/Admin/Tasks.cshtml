@model IEnumerable<DataPulse.Domain.Models.ProcessMaster>
<h1>Admin: Processes</h1>
<p><a asp-action="Servers" class="btn btn-link">Configure servers &amp; add jobs</a></p>
<table class="table">
    <thead><tr><th>Name</th><th>Description</th><th>Steps</th></tr></thead>
    <tbody>
    @foreach (var process in Model)
    {
        <tr>
            <td>@process.ProcessName</td>
            <td>@process.ProcessDescription</td>
            <td>@process.Steps.Count</td>
        </tr>
    }
    </tbody>
</table>
